{
  "name": "ocelot",
  "version": "1.0.0",
  "description": "Quizzing App on Express/Vue",
  "repository": {
    "type": "git",
    "url": "https://github.com/pniew/Ocelot"
  },
  "main": "src/index.ts",
  "scripts": {
    "lint": "eslint \"**/*.ts\"",
    "lint-fix": "eslint \"**/*.ts\" --fix",
    "build": "ttsc & cp -r src/views build/views;",
    "start": "node build/index.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --no-notify . # STFU!",
    "test": "mocha --require ts-node/register -r tsconfig-paths/register \"tests/**/*.ts\" --exit",
    "ui-test": "mocha --require ts-node/register -r tsconfig-paths/register \"ui-tests/**/*.ts\" --timeout 0 --exit"
  },
  "author": "ChomiczekGroup",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^4.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.6",
    "express-handlebars": "^4.0.4",
    "express-mysql-session": "^2.1.4",
    "express-session": "^1.17.1",
    "method-override": "^3.0.0",
    "mysql2": "^2.1.0",
    "nodemailer": "^6.4.16",
    "nodemailer-express-handlebars": "^4.0.0",
    "xlsx": "^0.16.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/chromedriver": "^81.0.0",
    "@types/cookie-session": "^2.0.39",
    "@types/express": "^4.17.6",
    "@types/express-fileupload": "^1.1.3",
    "@types/express-handlebars": "^3.1.0",
    "@types/express-mysql-session": "^2.1.1",
    "@types/express-session": "^1.17.0",
    "@types/method-override": "0.0.31",
    "@types/mocha": "^7.0.2",
    "@types/mysql2": "github:types/mysql2",
    "@types/node": "^13.13.5",
    "@types/nodemailer": "^6.4.0",
    "@types/selenium-webdriver": "^4.0.9",
    "@types/sinon": "^9.0.0",
    "@types/xlsx": "0.0.36",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "@zerollup/ts-transform-paths": "^1.7.17",
    "chai-http": "^4.3.0",
    "chromedriver": "^81.0.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-chai-expect": "^2.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "mocha": "^7.1.2",
    "selenium-webdriver": "^4.0.0-alpha.7",
    "sinon": "^9.0.2",
    "ts-node": "^8.10.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.10",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "node tools/gitPrefixCommit.js",
      "pre-commit": "node tools/gitProtectBranches.js"
    }
  }
}
